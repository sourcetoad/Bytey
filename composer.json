{
    "name": "sourcetoad/bytey",
    "description": "A library for encoding Google Map's Encoded Polyline.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sourcetoad\\Bytey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sourcetoad\\Bytey\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "illuminate/collections": "^11.20"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "phpunit/phpunit": "^11"
    },
    "config": {
        "sort-packages": true
    }
}
